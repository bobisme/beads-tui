{
  "version": "1.0.13",
  "project": {
    "name": "beads-tui",
    "type": [
      "tui"
    ],
    "channel": "beads-tui",
    "defaultAgent": "beads-tui-dev",
    "installCommand": "just install"
  },
  "tools": {
    "beads": true,
    "maw": true,
    "crit": true,
    "botbus": true,
    "botty": true
  },
  "review": {
    "enabled": false,
    "reviewers": []
  },
  "pushMain": true,
  "agents": {
    "dev": {
      "model": "opus",
      "pause": 2,
      "timeout": 900,
      "maxLoops": 20
    },
    "worker": {
      "model": "haiku",
      "timeout": 600
    },
    "reviewer": {
      "model": "opus",
      "pause": 2,
      "timeout": 600,
      "maxLoops": 20
    }
  }
}
